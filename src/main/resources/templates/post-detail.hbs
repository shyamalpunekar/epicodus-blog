{{#partial "content"}}
   <p>Content:</p>
   <p>{{ blog.feelings }}</p>
   <h5>Created At:</h5>
   <p> {{ blog.createdAt }}</p>
   <h5>Post status:</h5>
   <p>
       {{#if blog.published }}
           <span class="glyphicon glyphicon-star" aria-hidden="true"></span>  Published
       {{else}}
           <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Draft
       {{/if}}
   </p>
{{/partial}}

{{> layout.hbs}}
